<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    app:layout_behavior="com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior"
    tools:context="com.shetj.diyalbume.main.view.MainActivity"
    tools:showIn="@layout/activity_main">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/iRecyclerView"
        android:layout_width="1dp"
        android:layout_height="match_parent"
        android:layout_marginEnd="11dp"
        android:layout_marginRight="11dp"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/bt_create"
        app:layout_constraintTop_toTopOf="parent"
        >
    </androidx.recyclerview.widget.RecyclerView>

    <Button
        android:id="@+id/bt_create"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="介绍"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/btn_play"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="自动play原理"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_custom"/>

    <Button
        android:id="@+id/btn_test_auto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="AS自动生成的代码效果"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_play"/>
    <Button
        android:id="@+id/btn_Service_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="DownloadService"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_test_auto"/>

    <Button
        android:id="@+id/btn_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="系统控件"
        app:layout_constraintStart_toEndOf="@+id/btn_test_auto"
        app:layout_constraintTop_toBottomOf="@+id/btn_fresco"/>


    <Button
        android:id="@+id/btn_ppt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="ppt"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Service_1"/>

    <Button
        android:id="@+id/btn_account"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="跳转微信"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_ppt"
        app:layout_constraintTop_toBottomOf="@+id/btn_Service_1"/>
    <Button
        android:id="@+id/btn_test_qmui"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="other(fragment)"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_account"
        app:layout_constraintTop_toBottomOf="@+id/btn_Service_1"/>
    <Button
        android:id="@+id/btn_download"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="download"
        android:textAllCaps="false"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_ppt"/>

    <Button
        android:id="@+id/btn_job_scheduler"
        android:layout_width="wrap_content"
        android:layout_height="47dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="job_scheduler"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_download"
        app:layout_constraintTop_toBottomOf="@+id/btn_ppt" />
    <Button
        android:id="@+id/btn_local_music"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="内容提供者"
        app:layout_constraintStart_toEndOf="@+id/btn_view"
        app:layout_constraintTop_toBottomOf="@+id/btn_scheme"/>
    <Button
        android:id="@+id/btn_custom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="自定义"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_behavior"/>

    <Button
        android:id="@+id/btn_behavior"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="behavior"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_jiami"/>

    <Button
        android:id="@+id/btn_openGL"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textAllCaps="false"
        android:text="GLTest"
        app:layout_constraintStart_toEndOf="@+id/bt_create"
        app:layout_constraintTop_toTopOf="parent"/>

    <Button
        android:id="@+id/btn_openGL3D"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:textAllCaps="false"
        android:text="GL3D"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_openGL"
        app:layout_constraintStart_toEndOf="@+id/btn_openGL"
        />
    <Button
        android:id="@+id/btn_Worker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:textAllCaps="false"
        android:text="Worker"
        app:layout_constraintBaseline_toBaselineOf="@+id/btn_openGL"
        app:layout_constraintStart_toEndOf="@+id/btn_openGL3D"
        />

    <Button
        android:id="@+id/btn_jiami"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="加密解密"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_tup"/>

    <Button
        android:id="@+id/btn_tup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/btn_behavior"
        android:layout_alignTop="@+id/btn_openGL"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="图文生成"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_treadPool"/>

    <Button
        android:id="@+id/btn_treadPool"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/bt_create"
        android:layout_alignTop="@+id/btn_openGL"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:text="线程池"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_create"/>

    <Button
        android:id="@+id/btn_animator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:text="基础动画"
        app:layout_constraintStart_toEndOf="@+id/btn_treadPool"
        app:layout_constraintTop_toBottomOf="@+id/btn_openGL"/>

    <Button
        android:id="@+id/btn_bluetooth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:textAllCaps="false"
        android:text="蓝牙Ble"
        app:layout_constraintBottom_toTopOf="@+id/btn_finger"
        app:layout_constraintStart_toEndOf="@+id/btn_tup"
        app:layout_constraintTop_toBottomOf="@+id/btn_animator"/>

    <Button
        android:id="@+id/btn_lottie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:textAllCaps="false"
        android:text="lottie+spinkit"
        app:layout_constraintStart_toEndOf="@+id/btn_animator"
        app:layout_constraintTop_toBottomOf="@+id/btn_openGL3D"/>
    <Button
        android:id="@+id/btn_sneaker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:textAllCaps="false"
        android:text="Sneaker"
        app:layout_constraintStart_toEndOf="@+id/btn_lottie"
        app:layout_constraintTop_toBottomOf="@+id/btn_openGL3D"/>
    <Button
        android:id="@+id/btn_finger"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="指纹"
        app:layout_constraintStart_toEndOf="@+id/btn_jiami"
        app:layout_constraintTop_toBottomOf="@+id/btn_bluetooth"
        />

    <Button
        android:id="@+id/btn_CustomTabs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="CustomTabsUrl"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_behavior"
        app:layout_constraintTop_toBottomOf="@+id/btn_finger"/>

    <Button
        android:id="@+id/btn_Aspect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="AOP - Aspect"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_custom"
        app:layout_constraintTop_toBottomOf="@+id/btn_CustomTabs"/>

    <Button
        android:id="@+id/btn_hotfix"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="通过热修复添加按钮"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_bluetooth"
        app:layout_constraintTop_toBottomOf="@+id/btn_lottie"/>
    <Button
        android:id="@+id/btn_x5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="腾讯X5+VasSonic"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_finger"
        app:layout_constraintTop_toBottomOf="@+id/btn_hotfix"/>

    <Button
        android:id="@+id/btn_notification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="notification"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_CustomTabs"
        app:layout_constraintTop_toBottomOf="@+id/btn_x5"/>
    <Button
        android:id="@+id/btn_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="百度地图"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_Aspect"
        app:layout_constraintTop_toBottomOf="@+id/btn_notification"/>

    <Button
        android:id="@+id/btn_fresco"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="Fresco+图片"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_play"
        app:layout_constraintTop_toBottomOf="@+id/btn_map"/>


    <Button
        android:id="@+id/btn_scheme"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="web_scheme"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_fresco"
        app:layout_constraintTop_toBottomOf="@+id/btn_map"/>

    <Button
        android:id="@+id/btn_ges"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="gestureDetector"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_Service_1"
        app:layout_constraintTop_toBottomOf="@+id/btn_view"/>

    <Button
        android:id="@+id/btn_open_luck"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginTop="5dp"
        android:text="AIDL打开LuckAPP"
        android:textAllCaps="false"
        app:layout_constraintStart_toEndOf="@+id/btn_ges"
        app:layout_constraintTop_toBottomOf="@+id/btn_view"/>
</androidx.constraintlayout.widget.ConstraintLayout>
